<% include partials/header %>

<h1>Logged in as Contestant <%= user.name %></h1>
<div class="pick-buttons" style="display: none;">
    <div class="flex-row" style="justify-content: center">
        <button class="socket-button" data-event="move-pick" data-event-data="up">
            <i class="fa fa-chevron-up"></i>
        </button>
    </div>
    <div class="flex-row" style="justify-content: center">
        <button class="socket-button" data-event="move-pick" data-event-data="left">
            <i class="fa fa-chevron-left"></i>
        </button>
        <button class="socket-button" data-event="lock-pick">
            <i class="fa fa-circle"></i>
        </button>
        <button class="socket-button" data-event="move-pick" data-event-data="right">
            <i class="fa fa-chevron-right"></i>
        </button>
    </div>
    <div class="flex-row" style="justify-content: center">
        <button class="socket-button" data-event="move-pick" data-event-data="down">
            <i class="fa fa-chevron-down"></i>
        </button>
    </div>
</div>

<div class="flex-row buzzer" onclick="()=>{}" style="justify-content: middle">
    <i class="fa fa-bell-slash"></i>
</div>

<a href="/logout">Logout</a>

<script src="js/contestant.bundle.js"></script>
<% include partials/footer %>
