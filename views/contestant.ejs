<% include partials/header %>

<h1>Logged in as Contestant <%= user.name %></h1>
<button class="socket-button" data-event="move-pick" data-event-data="up">up</button>
<button class="socket-button" data-event="move-pick" data-event-data="right">right</button>
<button class="socket-button" data-event="move-pick" data-event-data="down">down</button>
<button class="socket-button" data-event="move-pick" data-event-data="left">left</button>
<button class="socket-button" data-event="lock-pick">lock</button>
<button class="socket-button" data-event="buzzed">Buzz</button>

<a href="/logout">Logout</a>

<div id="message-box">Nothing</div>

<script src="contestant.bundle.js"></script>
<% include partials/footer %>
